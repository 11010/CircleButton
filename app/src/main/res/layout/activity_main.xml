<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="bodor.com.circlebutton.MainActivity">


    <com.cuboid.cuboidcirclebutton.CuboidButton
        android:text="hello"
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:cub_color = "@android:color/holo_blue_dark"
        app:cub_border_radius = "4dp"
        app:cub_hover_color = "#000"
        app:cub_icon = "@drawable/ic_launcher"
        app:cub_riple_effect = "true"
        app:cub_border_color = "#000"

        />

</RelativeLayout>
